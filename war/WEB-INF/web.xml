<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>StoreTour</servlet-name>
		<servlet-class>edu.cmu.tourout.StoreTourServlet</servlet-class>
	</servlet>
	<servlet>
	    <servlet-name>GetTours</servlet-name>
	    <servlet-class>edu.cmu.tourout.GetToursServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>StoreTour</servlet-name>
		<url-pattern>/storeTour</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
	    <servlet-name>GetTours</servlet-name>
	    <url-pattern>/getTours</url-pattern>
	</servlet-mapping>
</web-app>
